#
# Create a file called postgres.env and populate these values in that file for authentication
#

# Django debug mode. Turn off for production
DEBUG=True

# Django secret key. Change for production
SECRET_KEY=Some-Autogenerated-Secret-Key

# Postgres database settings
POSTGRES_DB=postgres-db-name
POSTGRES_USER=postgres-db-user
POSTGRES_PASSWORD=postgres-db-password
